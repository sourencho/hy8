function init_kerning()
    -- kerning
    kerning={}
    -- store kerning data in array
    for c,v in pairs(kdata) do
        kerning[c]=v
    end
end

function hy(str,x,y,c)
    local x0=x
    for i=1,#str do
        local char=sub(str,i,i)
        if char=="\n" then
            x0=x
            y+=6
        else
            print("\14"..char,x0,y,c)
            x0+=6
            if kerning[char] then
                x0-=kerning[char]
            end
        end
    end
end

-- core
function _init()
    init_kerning()
end

